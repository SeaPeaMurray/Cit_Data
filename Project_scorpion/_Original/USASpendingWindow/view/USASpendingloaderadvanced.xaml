<UserControl x:Class="USAspendingWindow.USASpendingloaderadvanced"
              xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:USAspendingWindow"
             mc:Ignorable="d"  
             xmlns:awe="http://schemas.awesomium.com/winfx"
             
    Height="722" Width="924" Loaded="UserControl_Loaded">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/USAspendingWindow;component/1024215-1366x768-%5bDesktopNexus_com%5d.jpg" />
        </Grid.Background>
        <Button  Content="Load" Height="23" HorizontalAlignment="Left" Margin="481,212,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click_1" />
        <Label Content="Pick the Fiscal Year, or nothing to default the current year." Height="36" HorizontalAlignment="Left" Margin="17,97,0,0" Name="label1" VerticalAlignment="Top" Width="335" Foreground="Snow" FontSize="18" />
        <ComboBox  Height="23" HorizontalAlignment="Left" Margin="17,147,0,0" Name="comboBox1" VerticalAlignment="Top" Width="120" />
        <Label Content="Pick the date of the year to update from, or nothing for default" HorizontalAlignment="Left" Margin="17,166,0,0" Name="label2" Foreground="Snow" Height="36" VerticalAlignment="Top" FontSize="18" />
        <Label Content="USASpending.gov downloader and database update" Height="63" HorizontalAlignment="Left" Margin="17,41,0,0" Name="label3" VerticalAlignment="Top" FontSize="24" Width="621" FontFamily="Bookman Old Style" Foreground="White" />
        <DatePicker  DisplayDate="{Binding DateTime.Now}" Height="25" HorizontalAlignment="Left" Margin="111,212,0,0" Name="datePicker1" VerticalAlignment="Top" Width="115" />
        <TextBox Name="textBox1" AcceptsReturn="True" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" HorizontalAlignment="Left" Width="141" Height="408" VerticalAlignment="Top" Margin="14,261,0,0" />
        <awe:WebControl Margin="168,261,12,0" Name="webControl1" ShowCreatedWebView="webControl1_ShowCreatedWebView" Height="408" VerticalAlignment="Top" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="481,147,0,0" Name="comboBox2" VerticalAlignment="Top" Width="260" />
        <Label Content="Agency" Height="36" HorizontalAlignment="Right" Margin="0,97,353,0" Name="label4" VerticalAlignment="Top" Width="68" Foreground="Snow" FontSize="18" />
        <Image Height="129" HorizontalAlignment="Left" Margin="673,12,0,0" Name="image1" Stretch="Fill" VerticalAlignment="Top" Width="200" Source="/USAspendingWindow;component/USA-icon.png" />
        <!--<CheckBox Height="16" HorizontalAlignment="Left" Margin="723,219,0,0" Name="checkBox1" VerticalAlignment="Top" />
        <Label Content="Truncate Table" FontSize="18" Foreground="Snow" Height="36" HorizontalAlignment="Left" Margin="673,177,0,0" Name="label5" VerticalAlignment="Top" Width="132" />-->
        <DatePicker DisplayDate="{Binding DateTime.Now}" Height="25" HorizontalAlignment="Left" Margin="330,211,0,0" Name="datePicker2" VerticalAlignment="Top" Width="115" />
        <Label Content="End" FontSize="18" Foreground="Snow" Height="36" HorizontalAlignment="Left" Margin="275,205,0,0" Name="label6" VerticalAlignment="Top" Width="49" />
        <Label Content="Start" FontSize="18" Foreground="Snow" Height="36" HorizontalAlignment="Left" Margin="52,205,0,0" Name="label7" VerticalAlignment="Top" Width="53" />
        <Label Content="WARNING!!!!! ADVANCED LOADING WINDOW!" Height="37" HorizontalAlignment="Left" Margin="135,12,0,0" Name="label8" VerticalAlignment="Top" Width="569" FontFamily="Arial" Foreground="Red" FontSize="20" />
    </Grid>
   
</UserControl>

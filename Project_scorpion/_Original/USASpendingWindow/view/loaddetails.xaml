<Window x:Class="USAspendingWindow.loaddetails"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Loading Log Details" Height="316" Width="894" Background="LightCyan" Loaded="Window_Loaded">
    <Grid>
        <DataGrid ItemsSource="{Binding logitems}" AutoGenerateColumns="True" Height="218" HorizontalAlignment="Left" Name="dataGrid1" VerticalAlignment="Top" Width="872">
            
            
        </DataGrid>
        <Button Command="{Binding ViewSwitchCommand}" CommandParameter="close" Content="Close" Height="23" HorizontalAlignment="Left" Margin="390,242,0,0" Name="button1" VerticalAlignment="Top" Width="75" />
    </Grid>
</Window>

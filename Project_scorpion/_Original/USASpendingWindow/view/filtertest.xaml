<Window x:Class="USAspendingWindow.filtertest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="filtertest" Height="456" Width="1047">
    <Grid>
        <DataGrid ItemsSource="{Binding logitems}" AutoGenerateColumns="False" Height="349" HorizontalAlignment="Left" Margin="12,68,0,0" Name="dataGrid1" VerticalAlignment="Top" Width="1013">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Vendor Name" Binding="{Binding vendorname}" />
                <DataGridTextColumn Header="Product Service Code" Binding="{Binding productorservicecode}" />
                <DataGridTextColumn Header="Principal NAICS Code" Binding="{Binding principalnaicscode}" />
                <DataGridTextColumn Header="Description of Contract Requirement" Binding="{Binding descriptionofcontractrequirement}" />
                

            </DataGrid.Columns>
        </DataGrid> 
        <Button Content="Close" Height="23" HorizontalAlignment="Left" Margin="20,20,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
    </Grid>
</Window>
